\i units.sql
CREATE TEMP TABLE units (u) AS
  VALUES
	-- unity
	('1'::unit),
	-- base units
	(meter()),
	(kilogram()),
	(second()),
	(ampere()),
	(kelvin()),
	(mole()),
	(candela()),
	(byte()),
	-- coherent derived units
	(hertz()),
	(newton()),
	(pascal()),
	(joule()),
	(watt()),
	(coulomb()),
	(volt()),
	(farad()),
	(ohm()),
	(siemens()),
	(weber()),
	(tesla()),
	(henry()),
	(lux()),
	(katal());
-- test multiplication
WITH i(i) AS (VALUES ('-2'::unit), ('-1'), ('0'), ('1'), ('2'))
SELECT
  u, i, u * i
FROM
  units CROSS JOIN i
