-- test i/o
SELECT
  a.u, b.u, (a.u::unit*b.u::unit)::text, (a.u::unit*b.u::unit)::text::unit
FROM
  units a CROSS JOIN units b
WHERE (a.u::unit*b.u::unit)::text != (a.u::unit*b.u::unit)::text::unit::text;
